<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>ServletInitPhase</display-name>
  <welcome-file-list>
    <welcome-file>Home.html</welcome-file>
    <welcome-file>MyHome.html</welcome-file>
  </welcome-file-list>
 <servlet>
	<servlet-name>MyCompany</servlet-name>
	<servlet-class>com.servlet.CompanyServlet</servlet-class>
	<init-param>
		<param-name>companyName</param-name>
		<param-value>Neosoft Technologies</param-value>
	</init-param>
	<init-param>
	<param-name>headOffice</param-name>
	<param-value>Ruby Tower, Dadar</param-value>
	</init-param>
</servlet>
 <servlet-mapping>
 	<servlet-name>MyCompany</servlet-name>
 	<url-pattern>/Company/MyCompanyServlet</url-pattern>
 </servlet-mapping>
 <context-param>
	 <param-name>city</param-name>
	 <param-value>mumbai</param-value>
 </context-param>
 
</web-app>